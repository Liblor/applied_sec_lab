[Unit]
Description=Applied Security Lab CertServer

[Service]
WorkingDirectory=/home/{{ CORECA_UNAME }}/app
ExecStart=/usr/bin/dotnet /home/{{ CORECA_UNAME }}/app/CertServer.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
User={{ CORECA_UNAME }}
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
