[Unit]
Description=Applied Security Lab WebServer

[Service]
WorkingDirectory=/home/{{ UNAME }}/app
ExecStart=/usr/bin/dotnet /home/{{ UNAME }}/app/WebServer.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
User={{ UNAME }}
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
